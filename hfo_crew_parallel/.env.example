# HFO Crew Parallel Configuration
# Copy this file to .env and fill in your API keys

# Required: OpenAI API key for CrewAI
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Additional API keys
# ANTHROPIC_API_KEY=your_anthropic_key_here
# LANGCHAIN_API_KEY=your_langchain_key_here

# Crew Configuration
CREW_MODEL_NAME=gpt-4o-mini
CREW_TEMPERATURE=0.1
CREW_MAX_ITER=5

# Exploration vs Exploitation ratio (should sum to 1.0)
EXPLORE_RATIO=0.6
EXPLOIT_RATIO=0.4

# Blackboard path
BLACKBOARD_PATH=hfo_blackboard/obsidian_synapse_blackboard.jsonl
